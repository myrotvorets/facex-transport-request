{
  "name": "@myrotvorets/facex-transport-request",
  "version": "3.2.0",
  "description": "Native Node request transport for FaceX",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/lib/index.d.ts",
      "require": "./dist/cjs/lib/index.js",
      "import": "./dist/esm/lib/index.js",
      "default": "./dist/esm/lib/index.js"
    }
  },
  "main": "./dist/cjs/lib/index.js",
  "scripts": {
    "build": "tsc -b ./tsconfig.esm.json ./tsconfig.cjs.json ./tsconfig.types.json; echo '{ \"type\": \"module\" }' > ./dist/esm/lib/package.json; echo '{ \"type\": \"commonjs\" }' > ./dist/cjs/lib/package.json",
    "lint": "eslint -f gha .",
    "lint:fix": "eslint --fix .",
    "pretest": "npm run lint",
    "test": "jest",
    "prepare": "npm run build"
  },
  "files": [
    "dist/cjs/lib/*.js",
    "dist/cjs/lib/*.js.map",
    "dist/cjs/lib/package.json",
    "dist/esm/lib/*.js",
    "dist/esm/lib/*.js.map",
    "dist/esm/lib/package.json",
    "dist/types/lib/*.d.ts"
  ],
  "keywords": [],
  "author": "Myrotvorets <support@myrotvorets.center> (https://myrotvorets.center/)",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@myrotvorets/eslint-config-myrotvorets-ts": "^2.2.5",
    "@myrotvorets/facex-base": "^2.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": ">= 12",
    "eslint-formatter-gha": "^1.2.0",
    "jest": "^29.0.0",
    "jest-github-actions-reporter": "^1.0.3",
    "jest-sonar-reporter": "^2.0.0",
    "nock": "^13.2.2",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@myrotvorets/facex-base": "^2.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myrotvorets/facex-transport-request.git"
  },
  "bugs": {
    "url": "https://github.com/myrotvorets/facex-transport-request/issues"
  },
  "homepage": "https://github.com/myrotvorets/facex-transport-request#readme"
}
